pipeline {
    agent any
    stages {
        stage('Test for Ubuntu') {
            steps {
                echo "Test Ubuntu"
                sleep 10
            }
        }
        stage('Test for Windows') {
            steps {
                echo "Test Windows"
                sleep 5
            }
        }
        stage('Test for Extra OS') {
            steps {
                echo "Test Extra OS"
                sleep 10
            }
        }
    }
}
